fn returns_user(): User {
    return User{
        id: 4,
    };
}

fn get_user_id(): i64 {
    let user = returns_user();
    user.id = 5;
    return user.id;
}

fn use_method(user: User): i64 {
    return user.get_id();
}

type User struct {
    id: i64,
}

impl User {
    fn new(id: i64): Self {
        return Self{
            id: id,
        };
    }

    fn get_id(self: Self): i64 {
        return self.id;
    }
}

